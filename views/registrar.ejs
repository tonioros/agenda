<% include Shared/layout %>
<div class="row" ng-controller="registrarUS">
  <div class="col-md-4 col-md-offset-4">
    <h2>Registrar</h2>
    <form ng-submit="registrar()">
      <div class="form-group">
        <input ng-model="nickUS" type="text" name="nick" id="nick" class="form-control"
        required placeholder="Ingrese su nick">
      </div>

      <div class="form-group">
        <input type="password" ng-model="passUS" name="contrasena" id="contrasena" class="form-control"
        required placeholder="Ingrese la contraseÃ±a">
      </div>

      <div class="form-group">
        <div class="btn-group">
          <button type="submit" class="btn btn-success">Registrar</button>
        </div>
        <h1>{{Mensaje}}</h1>
      </div>
    </form>
  </div>
</div>
<% include Shared/footer %>
