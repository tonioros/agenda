<% include Shared/layout %>
<div class="row" ng-controller="autenticarUS">
  <div class="col-md-4 col-md-offset-4">
    <h2>Iniciar Sesion</h2>
    <form class="form-group">
        <input type="text" ng-model="nickAUTH" class="form-control" placeholder="Usuario">
        <input type="password" ng-model="passAUTH" class="form-control" placeholder="Contraseña">
        <button type="button" ng-click="autenticar()"  class="btn btn-primary">Entrar</button>
   </div>
   <div class="alert alert-danger" ng-show="showErrorAGMO">
     <h3 class="alert-heading">¡Vaya!, pareces que tenemos problemas</h3>
     <p>{{errorAGMO}}</p>
   </div>
    </form>
  </div>
</div>
<% include Shared/footer %>
